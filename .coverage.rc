[run]
source = src
branch = True
omit = 
	static/*
	cmds
	docs
	output
	templates
	xml2db
    *urls*
    src/protoLib/localisation/*
    src/snippets/*
	src/ProtoExt/settings*
    src/rai/*
    src/ll/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
