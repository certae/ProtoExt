                  <DOMAINEDEVALEURS text="Les codes pour le cas du mort-né sont :&#10;&lt;table class=&quot;table_interieure&quot;&gt;&#10;&lt;tr&gt;&#10;&lt;th&gt;Code&lt;/th&gt;&#10;&lt;th&gt;Description&lt;/th&gt;&#10;&lt;/tr&gt;&#10;&lt;tr&gt;&#10;&lt;td&gt;1&lt;/td&gt;&#10;&lt;td&gt;masculin&lt;/td&gt;&#10;&lt;/tr&gt;&#10;&lt;tr&gt;&#10;&lt;td&gt;2&lt;/td&gt;&#10;&lt;td&gt;féminin&lt;/td&gt;&#10;&lt;/tr&gt;&#10;&lt;tr&gt;&#10;&lt;td&gt;9&lt;/td&gt;&#10;&lt;td&gt;indéterminé,inconnu&lt;/td&gt;&#10;&lt;/tr&gt;&#10;&lt;/table&gt; "/>
                </udps>
              </property>
              <property>
                <code>Type de naissance</code>
                <description text="Il s'agit d'une variable qui caractérise le nombre d'enfants nés d'un même accouchement, soit une naissance unique, double ou autres."/>
                <udps>
                  <DOCUMENTDEREFERENCE text="Document Description variables mise à jour 07-2007"/>
                </udps>
              </property>
              <property>
                <code>Date accouchement</code>
                <baseType>DATE</baseType>
                <length>8</length>
                <description text="La date de l'accouchement qui donne lieu à une mortinaissance. Cette valeur est communiquée au et connue par le DSQ."/>
                <udps>
                  <DOCUMENTDEREFERENCE text="Document Description variables mise à jour 07-2007"/>
                </udps>
              </property>
              <property>
                <code>Durée grossesse</code>
                <baseType>INTEGER</baseType>
                <length>2</length>
                <udps>
                  <DOCUMENTDEREFERENCE text="Document Description variables mise à jour 07-2007"/>
                </udps>
              </property>
              <property>
                <code>Date récepion bulletin</code>
                <baseType>DATE</baseType>
                <length>8</length>
                <udps>
                  <DOCUMENTDEREFERENCE text="Document Description variables mise à jour 07-2007"/>
                </udps>
              </property>
            </properties>
